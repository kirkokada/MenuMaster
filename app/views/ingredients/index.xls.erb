<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">ID</Data></Cell>
        <Cell><Data ss:Type="String">Name</Data></Cell>
        <Cell><Data ss:Type="String">Calories</Data></Cell>
        <Cell><Data ss:Type="String">Carbs</Data></Cell>
        <Cell><Data ss:Type="String">Protein</Data></Cell>
        <Cell><Data ss:Type="String">Fat</Data></Cell>
      </Row>
    <% @ingredients.each do |ingredient| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= ingredient.id %></Data></Cell>
        <Cell><Data ss:Type="String"><%= ingredient.name %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= ingredient.calories %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= ingredient.carbs %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= ingredient.protein %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= ingredient.fat %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>

